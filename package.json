    "@babel/preset-typescript": "^7.10.4",
    "@types/react-infinite-scroller": "^1.2.1",
    "babel-jest": "^26.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
